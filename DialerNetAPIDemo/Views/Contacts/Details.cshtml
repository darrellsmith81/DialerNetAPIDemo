@model DialerNetAPIDemo.Models.Contact

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>Contact</h4>
    <hr />
    @foreach(var column in Model.Columns)
    {
        if (column.Value != null && ! string.IsNullOrWhiteSpace(column.Value as string))
        {
            <dl class="dl-horizontal">
                <dt>@column.Key</dt>
                <dd>@column.Value</dd>
            </dl>
        }
    }
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.id, contactlist_id = Model.ContactList.id }) |
    @Html.ActionLink("Delete", "Delete", new { id = Model.id, contactlist_id = Model.ContactList.id }) |
    @Html.ActionLink("Back to List", "Index", new { contactlist_id = Model.ContactList.id })
</p>
